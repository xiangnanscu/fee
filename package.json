{
  "name": "fee",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "u": "ncu -u && npm install",
    "prerelease": "npm --no-git-tag-version version minor",
    "release": "npm run swc && push_option=release npm run push",
    "connect": "pscale connect lab dev --port 3309",
    "db:push":"prisma db push",
    "push": "git add . && git commit -am \"${msg:-push}\" && git push --push-option=${push_option:-nothing}"
  },
  "dependencies": {
    "@xiangnanscu/model": "^0.10.0",
    "@xiangnanscu/sql": "^0.7.0",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "core-js": "^3.24.1",
    "file-saver": "^2.0.5",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4",
    "vm-browserify": "^1.1.2",
    "vue": "^2.7.8",
    "webpack": "^5.74.0",
    "xlsx-template-compact": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@prisma/client": "^4.1.1",
    "@types/node": "^18.6.4",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "arraybuffer-loader": "^1.0.8",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "prisma": "^4.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vue-template-compiler": "^2.7.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/require-v-for-key": "off",
      "vue/attribute-hyphenation": 0,
      "no-unused-vars": "off",
      "vue/no-unused-vars": "off",
      "semi": [
        1,
        "always"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}